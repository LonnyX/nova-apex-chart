{
    "name": "lonnyx/nova-apex-chart",
    "description": "A Laravel Nova Apex Chart Component.",
    "keywords": [
        "laravel",
        "nova",
        "apex chart",
        "chart"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "LonnyX\\NovaApexChart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LonnyX\\NovaApexChart\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
